{
	"compilerOptions": {
		"importHelpers": false,

		"esModuleInterop": true,
		"noEmit": true,
		// "baseUrl": "./",
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Node",
		"resolveJsonModule": true,
		"strict": true,
		"lib": ["ESNext", "DOM", "DOM.Iterable"],
		"paths": {
			"#/*": ["./src-wdb-projects/types/*"]
			// "@/*": ["./src-wdb-projects/*"],
		}
	},

	"watchOptions": {
		"watchFile": "priorityPollingInterval",
		"excludeDirectories": ["**/node_modules/"]
	},
	"exclude": ["node_modules"],
	"include": [
		"src-wdb-projects",
		"src-types/utils",
		"src-wdb-projects/projects/PJ-my-project/VER-main/resources/default/.info.ts"
	],
	"compileOnSave": false
}
